#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    int m=5;
    int n=5;
    for(int c=0;c<m;c++)
    {
        for(int r=0;r<n;r++)
        {
            if(c==0||c==m-1||r==0||r==n-1)
            cout<<"* ";
            else
            cout<<"  ";
        }
        cout<<endl;
    }

    return 0;
}




output:
/tmp/lHHaMkYbRA.o
* * * * * 
*       * 
*       * 
*       * 
* * * * * 
